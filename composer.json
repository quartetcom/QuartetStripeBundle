{
    "name": "quartet/stripe-bundle",
    "description": "Integration of stripe into symfony2.",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "PHP": "~5.6|~7.0",
        "symfony/framework-bundle": "^2.7.6|~3.0",
        "cartalyst/stripe": "^2.0.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5.7"
    },
    "autoload": {
        "psr-4": {
            "Quartet\\Bundle\\StripeBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Quartet\\Bundle\\StripeBundle\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
