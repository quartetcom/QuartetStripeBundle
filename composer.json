{
    "name": "quartet/stripe-bundle",
    "description": "Integration of stripe into symfony2.",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "PHP": "~5.6|~7.0",
        "stripe/stripe-php": "^4.3.0",
        "symfony/dependency-injection": "^2.6.5|~3.0",
        "symfony/framework-bundle": "^2.7.6|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5.7",
        "symfony/monolog-bundle": "^2.11.2"
    },
    "autoload": {
        "psr-4": {
            "Quartet\\Stripe\\": "src/Stripe",
            "Quartet\\StripeBundle\\": "src/StripeBundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Quartet\\Stripe\\": "tests/Stripe",
            "Quartet\\StripeBundle\\": "tests/StripeBundle"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
